#include <iostream>
using namespace std;

int main (){//begin

//numeric Luas, P, L
double Luas, P, L;
//Display, "Masukkan Panjangnya : "
cout << "Masukkan Panjangnya : ";
//Accept P
cin >> P;
//Display, "Masukkan Lebarnya : ";
cout << "Masukkan Lebarnya : ";
//Accept L
cin >> L;
//Compute Luas = P*L
Luas = P*L;
//Display, "Luasnya : " + Luas
cout << "Luasnya : " << Luas;
}//end